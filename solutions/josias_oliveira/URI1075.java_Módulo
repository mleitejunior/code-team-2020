package Iniciante;
import java.util.Scanner;

public class URI1075 {
    public static void main(String[] args) {
        
        
        Scanner sc = new Scanner(System.in);
        
        int N = sc.nextInt();
        
       
            for(int i = 0;i<10000;i++){
            
/*            Foi utilizado o operador modulo, porque era necessario
              checar se o resto da divisao de i por N era 2. 
              E se for imprimi ele na tela.
*/                  
                    if(i % N == 2)
                    System.out.println(i);
                        
            }
        
    }
}
