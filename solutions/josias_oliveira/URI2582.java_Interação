package Iniciante;
import java.util.Scanner;

public class URI2582 {
    public static void main(String[] args) {
        
        
      Scanner sc = new Scanner(System.in);
        
        int c, x, y, soma;
        
        c = sc.nextInt();//Entrada que define a quantidade de casos//
        
       /*Foi utilizado o FOR, pois a primeira
        entrada é o  numero de testes que 
        seram tetados, essa entrada vai ser a 
        quantidade de veses que o programa
        vai exucuta. O int i, é o contador.*/
       
        for(int i = 0; i < c; i++)
            {
             x = sc.nextInt();
             y = sc.nextInt();
             soma = x + y;
            
            switch(soma){
            case 0:
            System.out.println("PROXYCITY");
            break;
            
            case 1:
            System.out.println("P.Y.N.G.");
            break;
            
            case 2:
            System.out.println("DNSUEY!");
            break;
            
            case 3:
            System.out.println("SERVERS");
            break;
            
            case 4:
            System.out.println("HOST!");
            break;
            
            case 5:
            System.out.println("CRIPTONIZE");
            break;
            
            case 6:
            System.out.println("OFFLINE DAY");
            break;
            
            case 7:
            System.out.println("SALT");
            break;
            
            case 8:
            System.out.println("ANSWER!");
            break;
            
            case 9:
            System.out.println("RAR?");
            break;
            
            case 10:
            System.out.println("WIFI ANTENNAS");
            
            
        }
                
     }
      
   }
}
